  "license": "MIT",
  "scripts": {
    "serve:all": "nx run-many --targets=serve --projects=scale-media-backend,scale-media-frontend --parallel",
    "serve:backend": "nx serve scale-media-backend",
    "serve:frontend": "nx serve scale-media-frontend",
    "build:all": "nx run-many --targets=build --projects=scale-media-backend,scale-media-frontend --parallel",
    "build:backend": "nx build scale-media-backend",
    "build:frontend": "nx build scale-media-frontend",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migration:run": "cd scale-media-backend && ts-node --project tsconfig.typeorm.json -r tsconfig-paths/register src/data-source.ts",
    "migration:revert": "cd scale-media-backend && npx typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:generate": "cd scale-media-backend && npx typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "migration:create": "cd scale-media-backend && npx typeorm-ts-node-commonjs migration:create"
  },
  "private": true,
  "devDependencies": {
